
record(motor, "$(P):$(M)") {
    field(DTYP, "Soft Channel")         # Soft Channel Motor
    field(DINP,  "$(P):$(M):MOVING")    # Motor Moving 
#    field(DOL,  "$(P):$(M):VAL")        # Desired Outplut Loc 
##    field(OMLS,  "$(P):$(M):")          #  
    field(OUT,  "$(P):$(M):DVAL PP MS")       # Set motor position EGU
#    field(RDBL,  "$(P):$(M):RDBL")      # Readback Location 
    field(RINP,  "$(P):$(M):RMP")        # Raw Motor Position
##    field(RLINK,  "$(P):$(M):")          #  
    field(STOO,  "$(P):$(M):STOP.VAL NPP MS")      # Stop Motor 
}

